{
  "logger": {
    "level": "info",
    "timestamp": false
  },
  "esclient": {
    "apiVersion": "7.5",
    "hosts": [
      { "host": "elasticsearch" }
    ]
  },
  "elasticsearch": {
    "settings": {
      "index": {
        "refresh_interval": "10s",
        "number_of_replicas": "0",
        "number_of_shards": "3"
      }
    }
  },
  "acceptance-tests": {
    "endpoints": {
      "docker": "http://api:4000/v1/"
    }
  },
  "api": {
    "services": {
      "pip": { "url": "http://pip:4200" }
    }
  },
  "imports": {
    "adminLookup": {
      "enabled": true
    },
    "openstreetmap": {
      "download": [
        { "sourceURL": "http://download.geofabrik.de/south-america/brazil-latest.osm.pbf" },
        { "sourceURL": "http://download.geofabrik.de/south-america/chile-latest.osm.pbf" },
        { "sourceURL": "https://download.geofabrik.de/europe/portugal-latest.osm.pbf" }
      ],
      "leveldbpath": "/tmp",
      "datapath": "/data/openstreetmap",
      "import": [{"filename": "brazil-latest.osm.pbf"}, {"filename": "chile-latest.osm.pbf"}]
    },
    "whosonfirst": {
      "datapath": "/data/whosonfirst",
      "importPostalcodes": true,
      "importPlace": [
        "85633009",
        "85633057",
        "85633735"   
      ]
    }
  }
}
